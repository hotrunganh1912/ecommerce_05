<div class="container page-wrapper">
  <div class="page-inner">
    <div class="row">
      <%= render "products/slider_search_box" %>
      <%= render @products %>
      <% if @products.blank? %>
      <div class="alert alert-info">
        <p>Not found products</p>
      </div>
      <% end %>
    </div>
  </div>
</div>
<%= will_paginate %>
